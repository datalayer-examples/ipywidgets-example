{
  "name": "@datalayer-examples/ipywidgets-example",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/datalayer-examples/ipywidgets-example",
  "bugs": {
    "url": "https://github.com/datalayer-examples/ipywidgets-example/issues"
  },
